<menu buttonRadius='65' buttonSpan='60' buttonWidth='90' buttonBg='bgs/button.xml'>
    
    <button angle='-90'
            icon='icons/delete.png'
            onClick='actions/shape_delete.xml' />

    <button icon='icons/details.png'
            onClick='actions/range_circle_dropdown.xml' />

    <button disabled='!{${editable}}'
            icon='icons/edit2.png'
            onClick='actions/range_circle_edit.xml' />

    <button icon='icons/pairing_line.png'
            onClick='actions/pairingline.xml'/>

    <button icon='icons/label.png'
            onClick='actions/label_toggle.xml'
            selected='${labels_on}'/>
    
    <button icon='icons/move.png'
            submenu='menus/mgrs_entry_submenu.xml'
            disableSwap='true'
            disableIconSwap='true'
            onClick='actions/move.xml'
            disabled='!{${movable}}' />

</menu>