<menu buttonRadius='65' buttonSpan='60' buttonWidth='90' buttonBg='bgs/button.xml'>
    
    <button angle='-90'
            icon='icons/delete.png'
            disabled='!{${removable}}'
            onClick='actions/remove.xml' />
    
    <button icon='icons/details.png'
            onClick='actions/bullseye_details.xml' />

    <button icon='icons/degs_m.png'
            submenu='menus/randb_bullseye_bearing_submenu.xml'
            disableSwap='true'/>

    <button icon='icons/to_from.png'
            onClick='actions/toggle_bullseye_to_from.xml'/>

    <button icon='icons/range_ring.png'
            selected='${rangeRingVisible}'
            onClick='actions/toggle_bullseye_rings.xml'/>

    <button icon='icons/move.png'
            submenu='menus/mgrs_entry_submenu.xml'
            disableSwap='true'
            disableIconSwap='true'
            onClick='actions/move.xml'
            disabled='!{${movable}}' />


</menu>
